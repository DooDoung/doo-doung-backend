{
  "name": "doo-doung-backend",
  "version": "1.0.0",
  "description": "Backend for DooDoung project",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "prepare": "husky",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:dev": "tsx prisma/dev-seed.ts",
    "start": "nest start",
    "dev": "nest start --watch",
    "build": "nest build",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:reset": "npx prisma migrate reset",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.13.0",
    "argon2": "^0.44.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "csv-parser": "^3.2.0",
    "fast-csv": "^5.0.5",
    "nanoid": "^5.1.5",
    "nestjs": "^0.0.1",
    "papaparse": "^5.5.3",
    "pnpm": "^10.15.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@types/papaparse": "^5.3.16",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  }
}
